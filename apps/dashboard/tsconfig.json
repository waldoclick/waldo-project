{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": ["@nuxt/types", "@nuxtjs/strapi"],
    "typeRoots": ["./node_modules/@types", "./types"],
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "../../node_modules/@vitejs/**/*",
    "../../node_modules/@vue/**/*",
    ".nuxt",
    ".output",
    "dist"
  ],
  "strapi": {
    "version": "5"
  }
}

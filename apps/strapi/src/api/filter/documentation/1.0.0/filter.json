{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "name": "Filter API",
    "description": "API to get filter data such as communes and categories",
    "termsOfService": "YOUR_TERMS_OF_SERVICE_URL",
    "contact": {
      "name": "API Support",
      "url": "YOUR_CONTACT_URL",
      "email": "YOUR_CONTACT_EMAIL"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "x-strapi-config": {
    "path": "/documentation",
    "showGeneratedFiles": true,
    "generateDefaultResponse": false,
    "plugins": ["@strapi/plugin-users-permissions"]
  },
  "servers": [
    {
      "url": "http://localhost:1337",
      "description": "Development server"
    },
    {
      "url": "YOUR_PRODUCTION_URL",
      "description": "Production server"
    }
  ],
  "paths": {
    "/filter/communes": {
      "get": {
        "description": "Get the list of communes",
        "tags": ["Filter"],
        "summary": "Get communes",
        "responses": {
          "200": {
            "description": "List of communes",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer"
                          },
                          "documentId": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          },
                          "region": {
                            "type": "object",
                            "properties": {
                              "id": {
                                "type": "integer"
                              },
                              "name": {
                                "type": "string"
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/filter/categories": {
      "get": {
        "description": "Get the list of categories",
        "tags": ["Filter"],
        "summary": "Get categories",
        "responses": {
          "200": {
            "description": "List of categories",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer"
                          },
                          "documentId": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          },
                          "slug": {
                            "type": "string"
                          },
                          "color": {
                            "type": "string"
                          },
                          "icon": {
                            "type": "object"
                          },
                          "count": {
                            "type": "integer"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  }
}
